model User {
  id        String       @id @default(uuid())
  username  String       @unique
  password  String
  firstName String
  lastName  String?
  createdAt DateTime     @default(now())
  color     ChatColor
  sessions  Session[]
  chats     ChatMember[]

  contacts        Contact[] @relation(name: "ContactToOwner")
  addedByContacts Contact[] @relation(name: "ContactToUser")
}
